{
  "name": "qa-html-structure-capture",
  "displayName": "QA HTML Structure Capture & Compare",
  "description": "Extension for QA teams to capture HTML structure, save baselines, and compare changes to resolve locator issues",
  "version": "1.0.0",
  "publisher": "qa-tools",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Testing",
    "Other"
  ],
  "keywords": [
    "qa",
    "testing",
    "html",
    "dom",
    "locators",
    "automation"
  ],
  "activationEvents": [
    "onCommand:qa-html-capture.captureBaseline",
    "onCommand:qa-html-capture.compareStructure",
    "onCommand:qa-html-capture.viewDiff"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "qa-html-capture.captureBaseline",
        "title": "Capture HTML Baseline",
        "category": "QA HTML Capture"
      },
      {
        "command": "qa-html-capture.compareStructure",
        "title": "Compare HTML Structure",
        "category": "QA HTML Capture"
      },
      {
        "command": "qa-html-capture.viewDiff",
        "title": "View Structure Diff",
        "category": "QA HTML Capture"
      },
      {
        "command": "qa-html-capture.generateLocators",
        "title": "Generate Updated Locators",
        "category": "QA HTML Capture"
      },
      {
        "command": "qa-html-capture.iterateDiffResults",
        "title": "Iterate Diff Results",
        "category": "QA HTML Capture"
      },
      {
        "command": "qa-html-capture.captureFromWeb",
        "title": "Capture HTML from Web Page",
        "category": "QA HTML Capture"
      },
      {
        "command": "qa-html-capture.refresh",
        "title": "Refresh",
        "category": "QA HTML Capture"
      },
      {
        "command": "qa-html-capture.reconfigureProject",
        "title": "Reconfigure Project",
        "category": "QA HTML Capture"
      },
      {
        "command": "qa-html-capture.viewProjectSettings",
        "title": "View Project Settings",
        "category": "QA HTML Capture"
      },
      {
        "command": "qa-html-capture.viewCapturedMetadata",
        "title": "View Captured Metadata",
        "category": "QA HTML Capture"
      },
      {
        "command": "qa-html-capture.openProjectConfigFile",
        "title": "Open Project Config File",
        "category": "QA HTML Capture"
      },
      {
        "command": "qa-html-capture.reloadProjectConfigFromFile",
        "title": "Reload Project Config From File",
        "category": "QA HTML Capture"
      },
      {
        "command": "qa-html-capture.deleteProject",
        "title": "Delete Project",
        "category": "QA HTML Capture"
      },
      {
        "command": "qa-html-capture.clearWorkspaceStorage",
        "title": "Clear All Data",
        "category": "QA HTML Capture"
      },
      {
        "command": "qa-html-capture.openFile",
        "title": "Open File",
        "category": "QA HTML Capture"
      },
      {
        "command": "qa-html-capture.refreshFiles",
        "title": "Refresh Files",
        "category": "QA HTML Capture"
      },
      {
        "command": "qa-html-capture.openFolder",
        "title": "Open Folder",
        "category": "QA HTML Capture"
      },
      {
        "command": "qa-html-capture.stopBrowserMonitoring",
        "title": "Stop Browser Monitoring",
        "category": "QA HTML Capture"
      },
      {
        "command": "qa-html-capture.testBrowserLaunch",
        "title": "Test Browser Launch",
        "category": "QA HTML Capture"
      },
      {
        "command": "qa-html-capture.addTestInteraction",
        "title": "Add Test Interaction",
        "category": "QA HTML Capture"
      },
      {
        "command": "qa-html-capture.testNewBrowserWindow",
        "title": "Test New Browser Window",
        "category": "QA HTML Capture"
      },
      {
        "command": "qa-html-capture.testIncognitoBrowser",
        "title": "Test Incognito Browser",
        "category": "QA HTML Capture"
      },
      {
        "command": "qa-html-capture.clearWorkspaceStorage",
        "title": "Clear Workspace Storage",
        "category": "QA HTML Capture"
      },
      {
        "command": "qa-html-capture.switchProject",
        "title": "Switch Project",
        "category": "QA HTML Capture"
      },
      {
        "command": "qa-html-capture.showCurrentProject",
        "title": "Show Current Project",
        "category": "QA HTML Capture"
      },
      {
        "command": "qa-html-capture.showUploadedFile",
        "title": "Show Uploaded Test Cases File",
        "category": "QA HTML Capture"
      },
      {
        "command": "qa-html-capture.listProjects",
        "title": "List All Projects",
        "category": "QA HTML Capture"
      },
      {
        "command": "qa-html-capture.openBaselineFile",
        "title": "Open Baseline File",
        "category": "QA HTML Capture"
      },
      {
        "command": "qa-html-capture.showBaselinesFolder",
        "title": "Show Baselines Folder",
        "category": "QA HTML Capture"
      },
      {
        "command": "qa-html-capture.clearAllBaselines",
        "title": "Clear All Baselines",
        "category": "QA HTML Capture"
      },
      {
        "command": "qa-html-capture.refreshPanel",
        "title": "Refresh Panel",
        "category": "QA HTML Capture"
      },
      {
        "command": "qa-html-capture.selectBaseline",
        "title": "Select Baseline",
        "category": "QA HTML Capture"
      },
      {
        "command": "qa-html-capture.compareWithBaseline",
        "title": "Compare with Baseline",
        "category": "QA HTML Capture"
      },
      {
        "command": "qa-html-capture.generateLocatorReport",
        "title": "Generate Locator Report",
        "category": "QA HTML Capture"
      },
      {
        "command": "qa-html-capture.openPanel",
        "title": "Open QA HTML Tools Panel",
        "category": "QA HTML Capture"
      },
      {
        "command": "qa-html-capture.showBaselinesFolder",
        "title": "Show Baselines Folder",
        "category": "QA HTML Capture"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "qa-html-capture.captureBaseline",
          "when": "editorLangId == html",
          "group": "qa-html-capture"
        },
        {
          "command": "qa-html-capture.compareStructure",
          "when": "editorLangId == html",
          "group": "qa-html-capture"
        }
      ],
      "view/title": [
        {
          "command": "qa-html-capture.refreshPanel",
          "when": "view == qa-html-capture-panel",
          "group": "navigation"
        }
      ],
      "view/item/context": []
    },
    "views": {
      "qa-html-capture": [
        {
          "id": "qa-html-capture-panel",
          "name": "QA HTML Tools",
          "when": "true"
        },
        {
          "id": "qa-html-capture-baselines",
          "name": "Baselines",
          "when": "true"
        },
        {
          "id": "qa-html-capture-files",
          "name": "Project Files",
          "when": "true"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "qa-html-capture-baselines",
        "contents": "Welcome to QA HTML Capture!\n\n[Capture Baseline](command:qa-html-capture.captureBaseline)\n[Compare Structure](command:qa-html-capture.compareStructure)\n[Generate Locators](command:qa-html-capture.generateLocators)\n[Web Capture](command:qa-html-capture.captureFromWeb)\n\nGet started by capturing your first baseline!",
        "when": "true"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "qa-html-capture",
          "title": "QA HTML Capture",
          "icon": "$(diff)"
        }
      ]
    },
    "configuration": {
      "title": "QA HTML Capture",
      "properties": {
        "qa-html-capture.projectDetails": {
          "type": "object",
          "description": "Project configuration details for QA HTML Capture",
          "default": null
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "diff": "^5.1.0",
    "jsdom": "^22.1.0",
    "cheerio": "^1.0.0-rc.12"
  }
}
